{
  "license": "MIT",
  "main": "dist/main.js",
  "scripts": {
    "build": "NODE_ENV=production webpack -p --display errors-only",
    "build:renderer": "webpack-dev-server --port 8080 --config ./config/webpack.renderer.ts",
    "build:main": "webpack --config ./config/webpack.main.ts -w",
    "build:watch": "concurrently 'yarn build:renderer' 'yarn build:main'",
    "start": "electron ./dist/main.js",
    "prettier": "prettier --list-different './**/*.{ts,tsx,js,jsx,json}'",
    "prettier:write": "prettier --write './**/*.{ts,tsx,js,jsx,json}'",
    "tslint": "tslint --project .",
    "tslint:write": "tslint --project . --fix",
    "test:integration": "mocha",
    "lint": "yarn prettier && yarn tslint",
    "package": "electron-packager . CIWatcher --platform=darwin --arch=x64 --prune --asar --ignore='node_modules' 'src' 'test' --out package",
    "package:all": "electron-packager . CIWatcher --all"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@material-ui/core": "^3.7.0",
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/react": "^16.7.17",
    "@types/react-dom": "^16.0.11",
    "@types/react-router": "^4.4.3",
    "@types/webpack": "^4.4.21",
    "babel-loader": "^8.0.4",
    "concurrently": "^4.1.0",
    "css-loader": "^2.0.1",
    "electron": "4.0.0",
    "electron-packager": "^13.0.1",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^5.2.0",
    "prettier": "^1.15.3",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-router": "^4.3.1",
    "spectron": "^5.0.0",
    "style-loader": "^0.23.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.0",
    "typeface-roboto": "^0.0.54",
    "typescript": "^3.2.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "dependencies": {
    "file-loader": "^3.0.1"
  }
}
