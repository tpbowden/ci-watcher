{
  "license": "MIT",
  "scripts": {
    "build": "NODE_ENV=production webpack -p --silent",
    "build:renderer": "webpack-dev-server --port 8080 --config ./config/webpack.renderer.js",
    "build:main": "webpack --config ./config/webpack.main.js -w",
    "build:watch": "concurrently 'yarn build:renderer' 'yarn build:main'",
    "start": "electron ./dist/main.js",
    "prettier": "prettier --list-different './**/*.{ts,tsx,js,jsx,json}'",
    "prettier:write": "prettier --write './**/*.{ts,tsx,js,jsx,json}'",
    "tslint": "tslint --project .",
    "test": "mocha",
    "ci": "yarn prettier && yarn tslint && yarn build && yarn test"
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "mocha": "^5.0.1",
    "prettier": "^1.10.2",
    "spectron": "^3.8.0",
    "tslint": "^5.9.1",
    "webpack-dev-server": "^3.0.0-beta.1"
  },
  "dependencies": {
    "@types/react": "^16.0.38",
    "@types/react-dom": "^16.0.4",
    "@types/react-router": "^4.0.22",
    "@types/recompose": "^0.24.5",
    "css-loader": "^0.28.10",
    "electron": "1.8.2",
    "electron-packager": "^11.0.1",
    "material-ui": "^1.0.0-beta.34",
    "material-ui-icons": "^1.0.0-beta.17",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router": "^4.2.0",
    "recompose": "^0.26.0",
    "style-loader": "^0.20.2",
    "ts-loader": "^4.0.0",
    "typescript": "^2.7.2",
    "webpack": "^4.0.0",
    "webpack-cli": "^2.0.8"
  }
}
